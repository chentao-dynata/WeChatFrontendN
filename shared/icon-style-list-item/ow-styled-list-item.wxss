/* shared/icon-style-list-item/ow-styled-list-item.wxss */
.icon_row{
  border: 1px solid rgb(233, 237, 238);
  width: 94%;
  display: flex;
  flex-direction: row;
  vertical-align: middle;
  height: 8vh;
  justify-content:space-between;
}
.icon_row image{
  width:8vh;
  height: 6vh;
}

.icon_row .texts {
  display: flex;
  flex-direction: column;
  margin: 35rpx 120rpx;
}
.icon_row .texts label{
  display: block;
  line-height: 30rpx;
}
.icon_row .texts label.small_grey{
  color: grey;
  font-size: 25rpx;
}

