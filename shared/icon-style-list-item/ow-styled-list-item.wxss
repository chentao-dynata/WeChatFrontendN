.icon_row{
  box-shadow: 1rpx 1rpx 1rpx 1rpx #888888;
  display: flex;
  flex-direction: row;
  vertical-align: middle;
  justify-content:space-between;
}

.icon_row .texts {
  display: flex;
  flex-direction: column;
  margin: 20rpx 120rpx;
}
.icon_row .texts label{
  display: block;
  line-height: 30rpx;
}
.icon_row .texts input{
  width: 100%;
}


.icon_row .texts label.small_grey{
  color: grey;
  font-size: 25rpx;
}



