<!--shared/active-suvey-item/active-survey-item.wxml-->
<view class="active_survey">
  <view class="left">
    <label for="" class="survey_name">#hashA {{surveyName}}</label>
    <label for="" class="reward" style="color:{{hisMod?'rgb(74, 148, 208)':'red'}}">Reward:60 points</label>
    <button class="btn_normal {{hisMod?'hidden':'show'}}" data-url="{{survey.entryLink}}"
                  data-trusted="{{survey.entryLink}}">{{translation.takeSurvey}}</button>
    <label style="display:{{hisMod?'':'none'}};">Date:12/05/2019</label>
  </view>
  <view class="clock">
    <text>{{survey.lengthOfInterview}} {{translation.mins}}</text>
  </view>

</view>
